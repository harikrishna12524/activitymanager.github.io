<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
>
<Dialog title="Import Sheet">
    <Text text='If you already have an old sheet, please give us link to import.'></Text>
    <Input placeholder="sheet link" width="90%"></Input>
    <beginButton>
        <Button text="Ok" press=".onCloseDialog"></Button>
    </beginButton>
    <endButton>
        <Button text="Cancel" press=".onCloseDialog"></Button>
    </endButton>
</Dialog>
</c:FragmentDefinition>